<?xml version="1.0" encoding="utf-8"?>
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">

	<Type Name="aiString">
		<DisplayString>{data,s}</DisplayString>
		<Expand>
			<Item Name="[data]">data</Item>
		</Expand>
	</Type>

	<Type Name="aiMaterialProperty">
		<DisplayString Condition="mType==1">{mKey} {*(float*)mData}</DisplayString>
		<DisplayString Condition="mType==2">{mKey} {*(double*)mData}</DisplayString>
		<DisplayString Condition="mType==3">{mKey} {mData+4,s}</DisplayString>
		<DisplayString Condition="mType==4">{mKey} {*(int*)mData}</DisplayString>
		<DisplayString Condition="mType>4">{mKey} {mData}</DisplayString>
		<Expand>
			<Item Name="[key]">mKey</Item>
			<Item Name="[value]" Condition="mType==1">*(float*)mData</Item>
			<Item Name="[value]" Condition="mType==2">*(double*)mData</Item>
			<Item Name="[value]" Condition="mType==3">mData+4,s</Item>
			<Item Name="[value]" Condition="mType==4">*(int*)mData</Item>
			<Synthetic Name="[value]" Condition="mType==5">
				<DisplayString>[size={mDataLength}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>mDataLength</Size>
						<ValuePointer>mData</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
			<Item Name="[semantic]">mSemantic</Item>
			<Item Name="[index]">mIndex</Item>
			<Item Name="[type]">mType</Item>
		</Expand>
	</Type>

	<Type Name="aiMaterial">
		<Expand>
			<Synthetic Name="[properties]">
				<DisplayString>[size={mNumProperties}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>mNumProperties</Size>
						<ValuePointer>mProperties</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
		</Expand>
	</Type>

	<Type Name="aiVector3D">
		<DisplayString>{x} {y} {z}</DisplayString>
		<Expand>
			<Item Name="[x]">x</Item>
			<Item Name="[y]">y</Item>
			<Item Name="[z]">z</Item>
		</Expand>
	</Type>

	<Type Name="aiVector4D">
		<DisplayString>{x} {y} {z} {w}</DisplayString>
		<Expand>
			<Item Name="[x]">x</Item>
			<Item Name="[y]">y</Item>
			<Item Name="[z]">z</Item>
			<Item Name="[w]">w</Item>
		</Expand>
	</Type>

	<Type Name="aiQuaternion">
		<DisplayString>{w} {x} {y} {z}</DisplayString>
		<Expand>
			<Item Name="[w]">w</Item>
			<Item Name="[x]">x</Item>
			<Item Name="[y]">y</Item>
			<Item Name="[z]">z</Item>
		</Expand>
	</Type>

	<Type Name="aiColor3D">
		<DisplayString>{r} {g} {b}</DisplayString>
		<Expand>
			<Item Name="[r]">r</Item>
			<Item Name="[g]">g</Item>
			<Item Name="[b]">b</Item>
		</Expand>
	</Type>

	<Type Name="aiColor4D">
		<DisplayString>{r} {g} {b} {a}</DisplayString>
		<Expand>
			<Item Name="[r]">r</Item>
			<Item Name="[g]">g</Item>
			<Item Name="[b]">b</Item>
			<Item Name="[a]">a</Item>
		</Expand>
	</Type>

	<Type Name="aiFace">
		<DisplayString>{mNumIndices}</DisplayString>
		<Expand>
			<ArrayItems>
				<Size>mNumIndices</Size>
				<ValuePointer>mIndices</ValuePointer>
			</ArrayItems>
		</Expand>
	</Type>

	<Type Name="aiVertexWeight">
		<DisplayString>{mVertexId} {mWeight}</DisplayString>
		<Expand>
			<Item Name="[vertexId]">mVertexId</Item>
