				<DisplayString>{a1} {a2} {a3} {a4}</DisplayString>
				<Expand>
					<Item Name="[a1]">a1</Item>
					<Item Name="[a2]">a2</Item>
					<Item Name="[a3]">a3</Item>
					<Item Name="[a4]">a4</Item>
				</Expand>
			</Synthetic>
			<Synthetic Name="[row 2]">
				<DisplayString>{b1} {b2} {b3} {b4}</DisplayString>
				<Expand>
