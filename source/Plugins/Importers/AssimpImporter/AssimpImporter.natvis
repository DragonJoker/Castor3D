	<Type Name="aiMeshMorphKey">
		<DisplayString>{mTime}</DisplayString>
		<Expand>
			<Item Name="[time]">mTime</Item>
			<Synthetic Name="[values]">
				<DisplayString>[size={mNumValuesAndWeights}]</DisplayString>
				<Expand>
					<ArrayItems>
