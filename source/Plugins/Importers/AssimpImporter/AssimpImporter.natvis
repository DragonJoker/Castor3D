	<Type Name="aiMesh">
		<DisplayString>{mName}</DisplayString>
		<Expand>
			<Item Name="[name]">mName</Item>
			<Item Name="[primitiveTypes]">mPrimitiveTypes</Item>
			<Item Name="[materialIndex]">mMaterialIndex</Item>
			<Item Name="[numUVComponents]">mNumUVComponents</Item>
			<Item Name="[morphingMethod]">mMethod</Item>
			<Item Name="[aabb]">mAABB</Item>
			<Synthetic Name="[vertices]" Condition="mVertices!=0">
				<DisplayString>[size={mNumVertices}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>mNumVertices</Size>
						<ValuePointer>mVertices</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
			<Synthetic Name="[normals]" Condition="mNormals!=0">
				<DisplayString>[size={mNumVertices}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>mNumVertices</Size>
						<ValuePointer>mNormals</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
			<Synthetic Name="[tangents]" Condition="mTangents!=0">
				<DisplayString>[size={mNumVertices}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>mNumVertices</Size>
						<ValuePointer>mTangents</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
			<Synthetic Name="[bitangents]" Condition="mBitangents!=0">
