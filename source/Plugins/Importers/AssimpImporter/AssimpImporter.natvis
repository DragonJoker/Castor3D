				<Expand>
					<Item Name="[a1]">a1</Item>
					<Item Name="[a2]">a2</Item>
					<Item Name="[a3]">a3</Item>
				</Expand>
			</Synthetic>
			<Synthetic Name="[row 2]">
				<DisplayString>{b1} {b2} {b3}</DisplayString>
				<Expand>
					<Item Name="[b1]">b1</Item>
					<Item Name="[b2]">b2</Item>
					<Item Name="[b3]">b3</Item>
				</Expand>
			</Synthetic>
			<Synthetic Name="[row 3]">
				<DisplayString>{c1} {c2} {c3}</DisplayString>
				<Expand>
					<Item Name="[c1]">c1</Item>
					<Item Name="[c2]">c2</Item>
					<Item Name="[c3]">c3</Item>
