	<Type Name="aiTexture">
		<DisplayString>{mFilename} {achFormatHint,s}</DisplayString>
		<Expand>
			<Item Name="[filename]">mFilename</Item>
			<Item Name="[formatHint]">achFormatHint</Item>
			<Item Name="[width]">mWidth</Item>
			<Item Name="[height]">mHeight</Item>
			<Synthetic Name="[data]" Condition="pcData!=0">
				<DisplayString>[size={(mWidth>0?mWidth:1)*(mHeight>0?mHeight:1)}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>(mWidth>0?mWidth:1)*(mHeight>0?mHeight:1)</Size>
						<ValuePointer>pcData</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
		</Expand>
	</Type>

	<Type Name="aiLight">
		<DisplayString>{mName}</DisplayString>
		<Expand>
			<Item Name="[name]">mName</Item>
			<Item Name="[type]">mType</Item>
			<Item Name="[colorDiffuse]">mColorDiffuse</Item>
			<Item Name="[colorSpecular]">mColorSpecular</Item>
			<Item Name="[colorAmbient]">mColorAmbient</Item>
			<Item Name="[position]" Condition="mType!=1">mPosition</Item>
			<Item Name="[direction]" Condition="mType!=2">mDirection</Item>
			<Item Name="[up]" Condition="mType!=2">mUp</Item>
			<Item Name="[attenuationConstant]" Condition="mType!=1">mAttenuationConstant</Item>
			<Item Name="[attenuationLinear]" Condition="mType!=1">mAttenuationLinear</Item>
			<Item Name="[attenuationQuadratic]" Condition="mType!=1">mAttenuationQuadratic</Item>
			<Item Name="[angleInnerCone]">mAngleInnerCone</Item>
			<Item Name="[angleOuterCone]">mAngleOuterCone</Item>
			<Item Name="[size]" Condition="mType==5">mSize</Item>
		</Expand>
	</Type>

