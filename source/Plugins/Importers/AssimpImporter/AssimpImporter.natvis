	<Type Name="aiTexel">
		<DisplayString>{r,x} {g,x} {b,x} {a,x}</DisplayString>
		<Expand>
			<Item Name="[r]">r,x</Item>
			<Item Name="[g]">g,x</Item>
			<Item Name="[b]">b,x</Item>
