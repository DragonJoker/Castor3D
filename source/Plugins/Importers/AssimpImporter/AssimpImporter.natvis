					<ArrayItems>
						<Size>mNumVertices</Size>
						<ValuePointer>mTextureCoords</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
			<Synthetic Name="[colors]" Condition="mColors!=0">
				<DisplayString>[size={mNumVertices}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>mNumVertices</Size>
						<ValuePointer>mColors</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
			<Synthetic Name="[faces]" Condition="mFaces!=0">
				<DisplayString>[size={mNumFaces}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>mNumFaces</Size>
						<ValuePointer>mFaces</ValuePointer>
					</ArrayItems>
				</Expand>
			</Synthetic>
			<Synthetic Name="[bones]" Condition="mBones!=0">
				<DisplayString>[size={mNumBones}]</DisplayString>
				<Expand>
					<ArrayItems>
						<Size>mNumBones</Size>
						<ValuePointer>mBones</ValuePointer>
					</ArrayItems>
				</Expand>
